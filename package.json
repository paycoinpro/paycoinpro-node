{
  "name": "paycoinpro",
  "version": "1.2.1",
  "description": "Official TypeScript SDK for PayCoinPro payment gateway",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "generate": "openapi-typescript https://paycoinpro.com/api/openapi -o src/types/openapi.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "paycoinpro",
    "crypto",
    "payment",
    "gateway",
    "usdt",
    "bitcoin",
    "ethereum",
    "sdk",
    "typescript"
  ],
  "author": "PayCoinPro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paycoinpro/paycoinpro-node.git"
  },
  "bugs": {
    "url": "https://github.com/paycoinpro/paycoinpro-node/issues"
  },
  "homepage": "https://github.com/paycoinpro/paycoinpro-node#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^16.5.0",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
